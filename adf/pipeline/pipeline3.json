{
	"name": "pipeline3",
	"properties": {
		"activities": [
			{
				"name": "load from blob",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "SnowflakeSink",
						"preCopyScript": "truncate raw.dummy_table;",
						"importSettings": {
							"type": "SnowflakeImportCopyCommand"
						}
					},
					"enableStaging": true,
					"stagingSettings": {
						"linkedServiceName": {
							"referenceName": "StageBlobCon",
							"type": "LinkedServiceReference"
						},
						"path": ""
					},
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['SGN_SL_NO']"
								}
							},
							{
								"source": {
									"path": "$['RECORD_IDENTIFIER']"
								}
							},
							{
								"source": {
									"path": "$['AB_UNIT']"
								}
							},
							{
								"source": {
									"path": "$['CODE_VERIFIER']"
								}
							},
							{
								"source": {
									"path": "$['PAYMENT_METHOD']"
								}
							},
							{
								"source": {
									"path": "$['DATA_LOAD_DATE']"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "blobjson",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "snowflakeds",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}