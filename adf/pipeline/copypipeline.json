{
	"name": "copypipeline",
	"properties": {
		"activities": [
			{
				"name": "Copy data from blob",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "SnowflakeSink",
						"preCopyScript": "truncate table raw.dummy_table;",
						"importSettings": {
							"type": "SnowflakeImportCopyCommand"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "jsonblob",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Snowflakeds",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}